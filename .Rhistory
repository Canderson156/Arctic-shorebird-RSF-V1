Zones <- unique(data$Region_code)
i = 1
utm <- paste0("+proj=utm +zone=", Zones[i], " +ellps=WGS84", sep = "")
Lambert <- "+proj=lcc +lat_1=-47.74 +lat_2=86.46 +lat_0=23.8 +lon_0=-172.54 +x_0=0 +y_0=0 +ellps=GRS80 +datum=NAD83 +units=m +no_defs"
data_WGS <- SpatialPointsDataFrame(data.frame(x = 0, y = 0), data[1,],
proj4string = CRS("+proj=longlat +ellps=WGS84"))[-1,]
data_Lam <- SpatialPointsDataFrame(data.frame(x = 0, y = 0), data[1,],
