i = 2
#Create dataframe of only data in current loop's UTM zone
subdf <- subset(data, Region_code == Zones[i])
#turn subdf into spatial object
coordinates(subdf) <- c("UTM_1_Easting", "UTM_1_Northing")
#Set CRS to current UTM zone
proj4string(subdf) <- CRS(utm)
plot(subdf)
#loop over possible utm zones, transform into WGS84, then bind rows to output
for (i in 1:length(Zones)) {
#Create proj4string for current UTM zone in loop
utm <- paste0("+proj=utm +zone=", Zones[i], " +ellps=WGS84", sep = "")
Lambert <- "+proj=lcc +lat_1=-47.74 +lat_2=86.46 +lat_0=23.8 +lon_0=-172.54 +x_0=0 +y_0=0 +ellps=GRS80 +datum=NAD83 +units=m +no_defs"
#If/else statement that creates object on first run of loop, and keeps output for the rest
if (i == 1) {
#Create empty spatial object with appropriate coordinate reference system
data_WGS <- SpatialPointsDataFrame(data.frame(x = 0, y = 0), data[1,],
proj4string = CRS("+proj=longlat +ellps=WGS84"))[-1,]
data_Lam <- SpatialPointsDataFrame(data.frame(x = 0, y = 0), data[1,],
proj4string = CRS(Lambert))[-1,]
} else {
data_WGS <- data_WGS
data_Lam <- data_Lam
}
#Create dataframe of only data in current loop's UTM zone
subdf <- subset(data, Region_code == Zones[i])
#turn subdf into spatial object
coordinates(subdf) <- c("UTM_1_Easting", "UTM_1_Northing")
#Set CRS to current UTM zone
proj4string(subdf) <- CRS(utm)
#Transform spatial object CRS to WGS84
subWGS <- spTransform(subdf, CRS("+proj=longlat +ellps=WGS84"))
subLam <- spTransform(subWGS, CRS(Lambert))
#Bind data to output
data_WGS <- rbind(data_WGS, subWGS)
data_Lam <- rbind(data_Lam, subLam)
#Clean workspace
rm(subdf, utm, subWGS, subLam)
}
plot(data_WGS)
plot(data_Lam)
